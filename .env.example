# Multi-Agent Wellness Assistant Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required - OpenAI API Key for AI agents
OPENAI_API_KEY=your_openai_api_key_here

# Optional - Additional AI model providers (for future enhancements)
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_API_KEY=your_google_api_key_here
GROQ_API_KEY=your_groq_api_key_here
HUGGINGFACEHUB_API_TOKEN=your_huggingface_token_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Database Configuration (usually no changes needed)
DATABASE_URL=sqlite:///wellness_app.db
CHAT_HISTORY_DB=sqlite:///history.db

# Application Settings
DEBUG=False
LOG_LEVEL=INFO